#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x653c593b9640 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
v0x653c593cbda0_0 .net "out_tb", 0 0, L_0x653c593cc040;  1 drivers
v0x653c593cbe70_0 .var "sel_test", 1 0;
v0x653c593cbf40_0 .var "w", 3 0;
S_0x653c593b97d0 .scope module, "mux" "mux_4x1" 2 7, 3 2 0, S_0x653c593b9640;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "i";
    .port_info 1 /INPUT 2 "sel";
    .port_info 2 /OUTPUT 1 "out";
v0x653c593b9a00_0 .net "i", 3 0, v0x653c593cbf40_0;  1 drivers
v0x653c593cbba0_0 .net "out", 0 0, L_0x653c593cc040;  alias, 1 drivers
v0x653c593cbc60_0 .net "sel", 1 0, v0x653c593cbe70_0;  1 drivers
L_0x653c593cc040 .part/v v0x653c593cbf40_0, v0x653c593cbe70_0, 1;
    .scope S_0x653c593b9640;
T_0 ;
    %vpi_call 2 14 "$dumpfile", "mux_4x1_wave.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x653c593b9640 {0 0 0};
    %vpi_call 2 16 "$monitor", "Time = %d,select = %b,i = %b, output = %b", $time, v0x653c593cbe70_0, v0x653c593cbf40_0, v0x653c593cbda0_0 {0 0 0};
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x653c593cbf40_0, 0, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x653c593cbe70_0, 0, 2;
    %delay 1, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x653c593cbe70_0, 0, 2;
    %delay 1, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x653c593cbe70_0, 0, 2;
    %delay 1, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x653c593cbe70_0, 0, 2;
    %delay 1, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "mux_4x1_testbench.v";
    "mux_4x1.v";
